<html>
  <body>
    <h1>Aaarto</h1>
    <svg
      version="1.1"
      width="300"
      height="300"
      xmlns="http://www.w3.org/2000/svg"
      id="art"
    >
      <rect width="100%" height="100%" fill="gray" />
      <circle cx="150" cy="100" r="80" fill="green" />
      <text x="150" y="125" font-size="50" text-anchor="middle" fill="white">
        ART 3
      </text>
    </svg>

    <button onclick="a()">Add an Aaarto</button>
    <%= hello %>
    <script>
      const a = () => {
        const el = document.getElementById('art')
        const req = new Request("/addfile", {
          method: "POST",
          body: JSON.stringify({ art: el.outerHTML }),
          headers: {"Content-Type":"application/json"}
        });
        fetch(req)
      };
    </script>
  </body>
</html>
